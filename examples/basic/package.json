  "scripts": {
    "build": "next build",
    "dev": "concurrently -r next pnpm:dev:generate",
    "dev:generate": "graphql-codegen --config codegen.yaml --watch \"src/**/!generated/**/*\" --watch \"!src/generated\"",
    "generate": "graphql-codegen --config codegen.yaml",
    "start": "next start"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/core": "^2.4.0",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@types/node": "^16.11.19",
    "@types/react": "^17.0.38",
    "graphql": "^16.2.0",
    "next": "^12.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.8"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "rq-gql": "workspace:^0.4.2"
  }
}
