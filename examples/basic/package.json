    "@types/node": "^17.0.23",
    "@types/react": "^18.0.1",
    "graphql": "^16.3.0",
    "next": "^12.1.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.34.19",
    "valtio": "1.5.2"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "rq-gql": "workspace:^0.4.3"
  }
